<hr>

<div class="due-date-override-headers clearfix" style="min-width:1118px !important;">
  <div class="section-header">
    <label id="overrides_course_section">
    {{#t "overrides.for"}}For{{/t}}
    </label>
  </div>

  <div class="lock-header">
    <label id="overrides_unlock_at">
    {{#t "overrides.available_from"}}Available From{{/t}}
    </label>
  </div>
  <div class="unlock-header">
    <label id="overrides_lock_at">
    {{#t "until"}}Until{{/t}}
    </label>
  </div>
  {{#if ENV.SHOW_CORRECT_ANSWER}}
      <div>
        <label id="overrides_show_correct_answer_at">
        Show Correct Answers at
        </label>
      </div>
      <div>
          <label id="overrides_hide_correct_answers_at">
          Hide Correct Answers at
          </label>
      </div>
    </div>
{{else}}
    <div class="correct_answer_at_css" style="display:none;>
        <label id="overrides_show_correct_answer_at">
        Show Correct Answers at
        </label>
      </div>
      <div class="correct_answer_at_css" style="display:none;>
          <label id="overrides_hide_correct_answers_at">
          Hide Correct Answers at
          </label>
      </div>
    </div>
{{/if}}
<ul class="due-date-overrides" style="min-width:1118px !important;"></ul>

<div class="due-date-override-footer">
  <button class="btn" id="add_due_date" style="{{hiddenIf shouldHideDueDate}}">
    <i class="icon-add"><span class=screenreader-only>Add</span></i>
    {{#t "overrides.due_date"}}Due Date{{/t}}
  </button>
</div>

<% dashboard ||= false %>
<% if current_enrollment.course_section.start_at == nil && current_enrollment.course_section.end_at == nil %>
    <% unless dashboard %>
        <li class="<%= 'unpublished' if current_enrollment.course.created? || current_enrollment.course.claimed? %> <%= current_enrollment.state_based_on_date %>">
    <% end %>
    <% if current_enrollment.workflow_state == "invited" %>
        <div class="<%= dashboard ? 'communication_message dashboard_notification alert alert-info' : 'reminder' %>">
          <h2><a href="<%= course_path(current_enrollment.course_id, :invitation => current_enrollment.uuid) %>"><%= current_enrollment.long_name %></a></h2>
          <div class="body">
            <%=  render :partial => 'shared/invitation', :object => current_enrollment %>
          </div>
        </div>
    <% else %>
        <% show_link = current_enrollment.course.grants_right?(@current_user, session, :read) %>
        <% if show_link %>
            <% if current_enrollment.type == "StudentEnrollment" %>
                <a href="<%= course_quizzes_path(current_enrollment.course_id) %>">
            <%else %>
                <a href="<%= course_path(current_enrollment.course_id) %>">
            <% end %>
        <% end %>
        <span class="name ellipsis" title="<%= current_enrollment.long_name %>">
          <%= current_enrollment.long_name %>
          <% if current_enrollment.admin? && (current_enrollment.course.created? || current_enrollment.course.claimed?) %>
            <%= image_tag "hammer.png", :title => t('course_unpublished', "This project hasn't been published yet"), :alt => "Unpublished", :class => "unpublished_icon" %>
          <% end %>
        </span>

        <% unless current_enrollment.course.enrollment_term.default_term? %>
            <span class="subtitle ellipsis"><%= current_enrollment.course.enrollment_term.name %></span>
        <% end %>

        <%
           message = t('enrolled_as_student', "Enrolled as a candidate")
           case current_enrollment.type
             when 'TeacherEnrollment'
               message = t('enrolled_as_teacher', "Enrolled as a hiring manager")
             when 'TaEnrollment'
               message = t('enrolled_as_ta', "Enrolled as a interviewer")
             when 'ObserverEnrollment'
               message = t('enrolled_as_observer', "Enrolled as an HR")
             when 'DesignerEnrollment'
               message = t('enrolled_as_designer', "Enrolled as a designer")
           end
        %>
        <span class="subtitle ellipsis"><%= message %></span>
        <% if show_link %></a><% end %>
    <% end %>
    <% unless dashboard %>

      </li>
<% end %>

<%else%>
    <% if Date.today.to_s.to_date < current_enrollment.course_section.start_at.to_s.to_date || Date.today.to_s.to_date > current_enrollment.course_section.end_at.to_s.to_date %>
        <div class="content-box pad-box-mini border border-trbl border-round" style="margin: 100px auto;width: 50%;background: aliceblue;">
          <h1>Assesments are unavaiable, please contact your administrator.</h1>
        </div>
    <%else%>
        <% unless dashboard %>
            <li class="<%= 'unpublished' if current_enrollment.course.created? || current_enrollment.course.claimed? %> <%= current_enrollment.state_based_on_date %>">
        <% end %>
        <% if current_enrollment.workflow_state == "invited" %>
            <div class="<%= dashboard ? 'communication_message dashboard_notification alert alert-info' : 'reminder' %>">
              <h2><a href="<%= course_path(current_enrollment.course_id, :invitation => current_enrollment.uuid) %>"><%= current_enrollment.long_name %></a></h2>
              <div class="body">
                <%=  render :partial => 'shared/invitation', :object => current_enrollment %>
              </div>
            </div>
        <% else %>
            <% show_link = current_enrollment.course.grants_right?(@current_user, session, :read) %>
            <% if show_link %>
                <% if current_enrollment.type == "StudentEnrollment" %>
                    <a href="<%= course_quizzes_path(current_enrollment.course_id) %>">
                <%else %>
                    <a href="<%= course_path(current_enrollment.course_id) %>">
                <% end %>
            <% end %>
            <span class="name ellipsis" title="<%= current_enrollment.long_name %>">
          <%= current_enrollment.long_name %>
              <% if current_enrollment.admin? && (current_enrollment.course.created? || current_enrollment.course.claimed?) %>
            <%= image_tag "hammer.png", :title => t('course_unpublished', "This project hasn't been published yet"), :alt => "Unpublished", :class => "unpublished_icon" %>
          <% end %>
        </span>

            <% unless current_enrollment.course.enrollment_term.default_term? %>
                <span class="subtitle ellipsis"><%= current_enrollment.course.enrollment_term.name %></span>
            <% end %>

            <%
               message = t('enrolled_as_student', "Enrolled as a candidate")
               case current_enrollment.type
                 when 'TeacherEnrollment'
                   message = t('enrolled_as_teacher', "Enrolled as a hiring manager")
                 when 'TaEnrollment'
                   message = t('enrolled_as_ta', "Enrolled as a interviewer")
                 when 'ObserverEnrollment'
                   message = t('enrolled_as_observer', "Enrolled as an HR")
                 when 'DesignerEnrollment'
                   message = t('enrolled_as_designer', "Enrolled as a designer")
               end
            %>
            <span class="subtitle ellipsis"><%= message %></span>
            <% if show_link %></a><% end %>
        <% end %>
        <% unless dashboard %>
            </li>
        <% end %>
    <% end %>
<% end %>